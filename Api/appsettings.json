{
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Fatal",
        "System": "Fatal"
      }
    }
  },
  "JwtTokenOption": {
    "Issuer": "MONITOR.CHINA",
    "Audience": "TestAudience",
    "ExpiresInMinutes": 2,
    "Key": "monitor_china_jwt_token_key"
  },
  "MonitorApiServiceSetting": {
    "ServiceAddress": "http://localhost:50665"
  },
  "ConnectionStrings": {
    "DefaultConnection": "server=localhost;uid=root;pwd=admin;database=SampleProject"
  }
}
